<h1>Mis artículos</h1>

<% @articles.each do |article| %>
    <div class="card" style="padding: 15px; max-width: 300px">
        <%= article.id %>
        <h3 class="text-uppercase"><%= article.title %></h3>
        <p class="text-secondary"><%= article.created_at%></p>
        <%= link_to "Ver más", article%>
    </div>
<%end%>